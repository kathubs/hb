local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local LocalPlayer = Players.LocalPlayer

-- üîë Keys relacionadas con "99 Nights"
local validKeys = {
    "STEAL-RM81", "STEAL-JV64", "STEAL-TQ37", "STEAL-PZ58",
"STEAL-LK90", "STEAL-HB23", "STEAL-NY76", "STEAL-CX41",
"STEAL-GD12", "STEAL-WF85", "STEAL-UE07", "STEAL-QB69",
"STEAL-SZ34", "STEAL-MA18", "STEAL-VD53", "STEAL-KT62",
"STEAL-YR49", "STEAL-PL71", "STEAL-XC26", "STEAL-AN95"
}

local usedKeys = {}

local discordLink = "https://discord.gg/8uJDJaYNvw"
local keyLink = "https://pasteroblox2.blogspot.com/2025/08/99-nights-in-forest.html"

local gui = Instance.new("ScreenGui", LocalPlayer:WaitForChild("PlayerGui"))
gui.Name = "NIGHTSUI"
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

local blur = Instance.new("BlurEffect")
blur.Size = 10
blur.Name = "NightsBlur"
blur.Parent = Lighting

local function closeUI()
	if Lighting:FindFirstChild("NightsBlur") then
		Lighting.NightsBlur:Destroy()
	end
	gui:Destroy()
end

-- Main Frame
local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 340, 0, 370)
frame.Position = UDim2.new(0.5, -170, 0.5, -185)
frame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
frame.BackgroundTransparency = 0.05
frame.Active = true
frame.Draggable = true
Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 12)
local stroke = Instance.new("UIStroke", frame)
stroke.Color = Color3.fromRGB(0, 200, 255)
stroke.Thickness = 2
stroke.Transparency = 0.1

-- Avatar
local avatar = Instance.new("ImageLabel", frame)
avatar.Size = UDim2.new(0, 70, 0, 70)
avatar.Position = UDim2.new(0.5, -35, 0, 10)
avatar.BackgroundTransparency = 1
avatar.Image = "https://www.roblox.com/headshot-thumbnail/image?userId="..LocalPlayer.UserId.."&width=100&height=100&format=png"

-- Title
local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, -20, 0, 30)
title.Position = UDim2.new(0, 10, 0, 85)
title.Text = "99 NIGHTS - SCRIPT LOADER"
title.Font = Enum.Font.GothamBold
title.TextSize = 18
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.BackgroundTransparency = 1

-- Key Input
local keyBox = Instance.new("TextBox", frame)
keyBox.Size = UDim2.new(0.8, 0, 0, 38)
keyBox.Position = UDim2.new(0.1, 0, 0, 125)
keyBox.PlaceholderText = "Enter your key here..."
keyBox.Font = Enum.Font.Gotham
keyBox.TextSize = 16
keyBox.Text = ""
keyBox.BackgroundColor3 = Color3.fromRGB(35, 35, 50)
keyBox.TextColor3 = Color3.fromRGB(255, 255, 255)
Instance.new("UICorner", keyBox).CornerRadius = UDim.new(0, 8)

-- Verify Key Button
local verifyBtn = Instance.new("TextButton", frame)
verifyBtn.Size = UDim2.new(0.8, 0, 0, 38)
verifyBtn.Position = UDim2.new(0.1, 0, 0, 175)
verifyBtn.Text = "üîí Verify Key"
verifyBtn.Font = Enum.Font.GothamBold
