-- Coming Soon Button
local comingSoonBtn = Instance.new("TextButton", frame)
comingSoonBtn.Size = UDim2.new(0.8, 0, 0, 34)
comingSoonBtn.Position = UDim2.new(0.1, 0, 0, 315)
comingSoonBtn.Text = "Coming Soon"
comingSoonBtn.Font = Enum.Font.Gotham
